{
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "no-unused-vars":[
      "warn"
    ],
    "no-undef": [
      "warn"
    ],
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "warn",
      "single"
    ],
    "semi": [
      "warn",
      "always"
    ]
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  }
}
